{
  "name": "collab-code",
  "displayName": "CollabCode",
  "description": "A collaborative code editing extension",
  "version": "0.0.1",
  "publisher": "team-collab",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:collab-code.helloWorld",
    "onCommand:collab-code.connect",
    "onCommand:collab-code.startServer"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "collab-code.helloWorld",
        "title": "Open CollabCode Chat"
      },
      {
        "command": "collab-code.connect",
        "title": "Connect to CollabCode Server"
      },
      {
        "command": "collab-code.startServer",
        "title": "Start CollabCode Server"
      }
    ],
    "configuration": {
      "title": "CollabCode",
      "properties": {
        "collab-code.serverUrl": {
          "type": "string",
          "default": "",
          "description": "WebSocket server URL for collaborative editing"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "dependencies": {
    "ip": "^1.1.8",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.5.10",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
